{
  "name": "express-combo",
  "description": "Combo handler for express applications",
  "version": "0.2.0",
  "author": "Caridy Patino <caridy@yahoo-inc.com> (http://github.com/caridy)",
  "contributors": [
    "alberto chan <albertoc@yahoo-inc.com>"
  ],
  "engines": {
    "node": ">=0.10.x",
    "npm": ">=1.2.10"
  },
  "dependencies": {
    "debug": "*",
    "express": ">=3.x",
    "fresh": "0.2.4",
    "mime": "1.2.11",
    "range-parser": "1.0.2",
    "send": "0.9.2"
  },
  "devDependencies": {
    "chai": "1.9.1",
    "express-handlebars": "1.1.x",
    "istanbul": "0.3.2",
    "jshint": "2.5.5",
    "mocha": "1.21.4",
    "mockery": "1.4.x",
    "xunit-file": "*",
    "yui-lint": "0.2.0"
  },
  "main": "index",
  "keywords": [
    "framework",
    "combo",
    "mojito",
    "modown",
    "static"
  ],
  "directories": {
    "lib": "./lib",
    "example": "./examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/express-combo.git"
  },
  "bugs": {
    "url": "https://github.com/yahoo/express-combo/issues"
  },
  "scripts": {
    "test": "node ./node_modules/istanbul/lib/cli.js cover -- ./node_modules/mocha/bin/_mocha tests/units/*.js --globals foo --reporter spec",
    "unit": "node ./node_modules/.bin/mocha tests/units/*.js --reporter spec",
    "lint": "node ./node_modules/.bin/jshint --config ./node_modules/yui-lint/jshint.json ./lib/*.js"
  },
  "yahoo": {
    "bugzilla": {
      "product": "Mojito",
      "component": "General"
    }
  }
